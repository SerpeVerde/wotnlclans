    "// start": "start will start the server for production",
    "start": "node Dev-False.js && node serv.js",
    "// dev": "start will start the server in dev mode",
    "dev": "node Dev-True.js && ./node_modules/nodemon/bin/nodemon.js serv.js",
    "// update": "a easy update sctipt for on the serverside",
    "update": "cp ./update/update.sh ../wotnlclans-update.sh && sh ../wotnlclans-update.sh && rm ../wotnlclans-update.sh",
    "// debug": "with this command it's will be easyer for me to debug via a browser",
    "debug": "node Dev-True.js && node --inspect serv.js"
  },
