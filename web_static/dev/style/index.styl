*
  border 0px
  margin 0px
  padding 0px

body
  font-size 18px
  background-color white
  color black
  font-family: 'Open Sans', sans-serif
  overflow-x hidden

.list
  padding 20px
  .actualData
    padding-top 10px
  .row
    padding 5px
    background-color #f7f5f5
    box-shadow 0px 10px 20px -10px rgba(0,0,0,0.3)
    margin-bottom 20px
    border-radius 10px
    display flex
    align-items center
    .icon
      .holder
        height 30px
        width 30px
        margin 10px
    .data
      display grid
      grid-template-columns 165px 165px 165px 165px 165px
      grid-template-rows 30px 30px
      .rating
        align-self center
        font-weight 800
        padding 0px 10px
      span
        display inline-block
        padding-right 5px
        font-weight 400
        color #3f3f3f
        

